<div class="user-box" style="margin-top: 100px">
    <div class="top-line"></div>
    
    <div style="margin-top:20px">
        <form action="/attend/make_round">
              <div class="form-group">  
                <label for="check_month">month</label>
                <input type="text" name="month" class="form-control" id="check_month" placeholder="몇 월">
              </div>
              
              <div class="form-group">
                <label for="check_week">week</label>
                <input type="text" name="week" class="form-control" id="check_week" placeholder="몇 주차">
              </div>
              
              <button type="submit" class="btn btn-default">확인</button>
        </form>
     </div> 
</div>